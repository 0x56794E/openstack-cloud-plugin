<j:jelly xmlns:j="jelly:core"
         xmlns:f="/lib/form">
    <f:entry title="Provider Name" field="providerName">
        <f:textbox/>
    </f:entry>
    <f:entry title="Identity" field="identity">
        <f:textbox/>
    </f:entry>
    <f:entry title="Credential" field="credential">
        <f:textbox/>
    </f:entry>
    <f:validateButton title="Test Connection" progress="${%Testing...}" method="testConnection" with="providerName,identity,credential" />
</j:jelly>
